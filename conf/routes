# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.DataDictionaryController.index

GET     /categorize                 controllers.DataDictionaryController.categorize(phrase:String)

GET    /update_dictionary          controllers.DataDictionaryController.updateDataDictionaryByPath(fileNameAndPath:String)

POST   /update_dictionary          controllers.DataDictionaryController.updateDataDictionary()

GET    /list_dictionary_content    controllers.DataDictionaryController.listDataDictionaryContent()

