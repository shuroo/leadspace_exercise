# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.DataDictionaryController.index

GET     /categorize                 controllers.DataDictionaryController.categorize(phrase:String)

GET    /replace_dictionary          controllers.DataDictionaryController.replaceDataDictionaryByPath(fileNameAndPath:String)

POST   /replace_dictionary          controllers.DataDictionaryController.replaceDataDictionary()

GET    /reset_dictionary            controllers.DataDictionaryController.resetDictionary()

GET    /list_dictionary_content    controllers.DataDictionaryController.listDataDictionaryContent()

